<html>
  <head>
      <link rel="shortcut icon" type="image/ico" href="/img/favicon.ico" />
      <title>mLink</title>
    <link rel="stylesheet/less" href="/less/style.less">
    <script src="/js/vendor/less.js"></script>
    <script src="/js/vendor/modernizr-min.js"></script>
  </head>
  <body>
    <div id="wrapper">
      <div id="main">
        <div id="mLinks">
            <div class="tdesign" style="background-color: #EAEBE3!important;">
                <div style="text-align: center;"><a href="http://mlinks.us/">
                <strong style="font-size: 30px;"> mLinks</strong></a></div>
                <div class="vc_single_image-wrapper vc_box_border_grey"><img style="width:100%" height="10" alt="line3" src="https://www.microhealthllc.com/wp-content/uploads/2013/08/line3.png" title="line3" class="vc_single_image-img "></div>
                <p style="font-size: 14px; margin: 0px 8px 18px 8px; text-align: justify;"></p>
            </div>
        </div>
          <script type="text/template" id="mLinks-form-template">
              <form action="javascript:window.mLinksFormSubmit();" method="post" id="mLinks-url-form" style="text-align: center;">
                  <br /><br />
                  <input class="url-input" type="text" url="url"> <br /><br /><br /><br />
                  <input type="submit" style="text-align: center;" value="Shorten Link" id="button1" />
              </form>
          </script>
          <script type="text/template" id="mLinks-vanity-template">
              <form action="javascript:window.mLinksVanitySubmit();" method="post" id="mLinks-url-form" style="text-align: center;">
                  <br />
                  <div class="url-display url-one">{{placeholder}}:</div>
                  <input class="url-input" id="vanity-url-input" required="true" placeholder="Enter url" type="url"> <br /><br />
                  <input type="submit" style="text-align: center;" value="Add" id="button1" />
                  <input type="button" style="text-align: center;" onclick="window.showFormTemplate();" value="Cancle" id="button1" />
              </form>
          </script>
          <script type="text/template" id="mLinks-display-template">
          <div class="mLinks-display">
              <br />
            <div class="url-display url-one"><a href="{{ urlOne }}">{{ urlOne }}</a></div>
            <div class="url-display url-two"><a href="{{ urlTwo }}">{{ urlTwo }}</a></div>
            <div class="url-display copy-to-clip">
                <br />
                <a id="copy-button" href="#">Copy</a>
                <input type="button" onclick="window.copyUrl('{{ urlTwo }}');" id="button1" value="Copy URL" />
                <input type="button" onclick="window.showVanityTemplate();" id="button1" value="Add Vanity URL" />
            </div>
          </div>
        </script>
          <script type="text/template" id="mLinks-redirect-template">
              <div class="mLinks-display">
                  <br />
                  <div class="url-display url-one"><a href="{{ urlOne }}">{{ urlOne }}</a></div>
                  <div class="url-display url-two"><a href="{{ urlTwo }}">{{ urlTwo }}</a></div>
              </div>
          </script>
          <script type="text/template" id="mLinks-vanity-display-template">
              <div class="mLinks-display">
                  <br>
                  <div class="url-display url-one"><a href="{{ sortUrl }}">{{ url }}</a></div>
                  <div class="url-display url-two"><a href="{{ sortUrl }}">{{ sortUrl }}</a></div>
                  <div class="url-display url-two"><a href="{{ sortUrl }}">{{ vanityUrl }}</a></div>
                  <div class="url-display copy-to-clip">
                      <br />
                      <input type="button" style="text-align: center;" onclick="window.showFormTemplate();" value="Ok" id="button1" />
                  </div>
              </div>
          </script>
        <script type="text/template" id="mLinks-unsafe-template">
            <div class="mLinks-display">
                <br /><br />
                <div class="url-display url-one">{{ urlOne }}</div>
                <div class="url-display error">{{ error }}</div>
                <br /><br /><br />
                <input type="button" id="button1" onclick="window.showFormTemplate();" value="Try Again" />
            </div>
        </script>
      </div>
    </div>

    <!-- Vars -->
    <script>window.mLinksSurl = "<%= surl %>";</script>

    <!-- Load library scripts -->
    <script src="/js/vendor/jquery-1.7.1.min.js"></script>
    <script src="/js/vendor/underscore-min.js"></script>
    <script src="/js/vendor/zclip.js"></script>
    <!-- Load page scripts -->
    <script src="/js/app.js"></script>
    <!-- Analytics -->
    <script type="text/javascript">

      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-28014986-1']);
      _gaq.push(['_trackPageview']);

      (function() {
       var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
       ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
       var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
       })();
</script>
</body>